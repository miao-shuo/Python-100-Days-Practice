for cock in range(20):                                                #公鸡最多能买100//5==20只
    for hen in range(33):                                             #母鸡最多能买100//3==33只
        chick = 100 - cock - hen                                      #小鸡数量==100只减公母鸡数量
        if cock * 5 + hen * 3 + chick/3 * 1 == 100:                   #数量*单价==总价
            print('公鸡%d只，母鸡%d只，小鸡%d只' % (cock,hen,chick))

